  "license": "MIT",
  "main": "build/index.js",
  "module": "build/esm/index.js",
  "scripts": {
    "prebuild": "eslint --fix --ignore-pattern \"**/__tests__/**/*.ts\" \"./src/**/*.ts\"",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "serve:node": "node example/node/index.js",
    "serve:browser": "node example/web/server.js",
    "typecheck": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.0"
  }
}
